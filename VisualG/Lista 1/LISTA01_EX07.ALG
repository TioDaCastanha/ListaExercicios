Algoritmo "Lista01_ex07"

Var

   num, dobro, terco:real

Inicio

      escreva("Digite um numero:")
      leia(num)
      
      dobro <- num*2
      terco <- num/3
      
      escreval("O dobro de", num, " é:", dobro)
      escreva("A terça parte de", num, " é:", terco:2:2)

Fimalgoritmo