Algoritmo "Lista01_ex12"

Var

   preco, promo: real


Inicio

      escreva("Informe o preço do produto:")
      leia(preco)
      
      promo <- preco - (preco*0.05)
      
      escreval("Valor do Produto:",preco)
      escreval("Valor com desconto de 5%:",promo)


Fimalgoritmo