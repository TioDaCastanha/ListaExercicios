Algoritmo "Lista01_ex13"

Var
   salario, novoSalario: real


Inicio

      escreva("Informe seu salário:")
      leia(salario)
      
      novoSalario <- salario + salario*0.15
      
      escreval("Seu salário anterior: R$", salario)
      escreval("Seu salário com aumento de 15%: R$", novoSalario)

Fimalgoritmo