Algoritmo "Lista01_ex04"

Var

   soma, num1, num2 :inteiro

Inicio

      escreva("Digite um numero:")
      leia(num1)
      
      escreva("Digite outro numero:")
      leia(num2)
      
      soma <- num1 + num2
      
      escreva("A soma entre", num1, " e", num2, " é:", soma)

Fimalgoritmo