Algoritmo "Lista01_ex05"

Var

   nota1, nota2, media: real

Inicio

      escreva("Digita Nota 1:")
      leia(nota1)
      
      escreva("Digita Nota 2:")
      leia(nota2)
      
      media <- (nota1 + nota2)/2
      
      escreva("A média entre", nota1, " e", nota2, " é:", media)

Fimalgoritmo