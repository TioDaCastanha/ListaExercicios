Algoritmo "Lista01_ex15"

Var

   dias:inteiro
   salario:real

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 

   escreva("Informe a quantidade de dias trabalhados:")
   leia(dias)
   
   salario <- dias*(8*25)
   
   escreva("Seu sálario será de: R$", salario)

Fimalgoritmo